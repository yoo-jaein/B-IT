	<div class="limiter">
		<div class="container-login100">
			<div class="wrap-login100">
				<!--<div class="login100-pic js-tilt" data-tilt>-->
				<!--	<img src="images/img-01.png" alt="IMG">-->
				<!--</div>-->

          <div class="card-content">
            <span class="login100-form-title">Member Login</span>
            <hr>
             <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
          <div class="p-2 field">
              <i class="fa fa-paper-plane"></i>
            <%= f.email_field :email, autocomplete: "email", placeholder: "Email" %><br />
          </div>
        
          <div class="p-2 field">
            <i class="fa fa-lock"></i>
            <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password" %>
          </div>
        
          <% if devise_mapping.rememberable? -%>
            <div class="p-2 field">
              <%= f.check_box :remember_me %>
              Remember
            </div>
          <% end -%>
        
          <div class="container-login100-form-btn p-2 actions">
            <button type="submit" name="commit" class="login100-form-btn">Login</button>
          </div>
          <% end %>
        
        <%= render "devise/shared/links" %>
              
          </div>
        </div>
				
				</div>
				</div>
	